{
  "name": "deliverymuchtest",
  "version": "1.0.0",
  "description": "delivery much test",
  "main": "dist/main.js",
  "module": "dist/main.js",
  "scripts": {
    "start": "node dist/main.js",
    "dev": "nodemon",
    "build": "yarn build:clean && tsc ; yarn postbuild",
    "build:clean": "rimraf dist",
    "postbuild": "copyfiles -u 1 -e \"**/*.ts\" \"src/**/*\" dist",
    "lint:fix": "npm run lint --fix",
    "lint": "eslint ./src --ext .ts",
    "test": "export NODE_ENV=TEST && yarn build && jest -i dist/__test__/",
    "deploy:docker": "docker-compose up"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@shelf/jest-mongodb": "^1.2.3",
    "amqplib": "^0.7.1",
    "body-parser": "^1.19.0",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "csvtojson": "^2.0.10",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "helmet": "^4.4.1",
    "mongoose": "^5.11.19",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "ts-mongoose": "^0.0.24",
    "typescript": "^4.2.3",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/mongoose": "^5.7.36",
    "@typescript-eslint/parser": "^4.17.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "mongodb-memory-server": "^6.9.6",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.3",
    "ts-node-dev": "^1.0.0"
  }
}
